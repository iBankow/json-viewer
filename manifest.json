{
  "manifest_version": 3,
  "name": "Meu JSON Viewer Personalizado",
  "version": "1.0",
  "description": "Formata e colore arquivos JSON automaticamente.",
  "content_scripts": [
    {
      "matches": ["http://*/api/*", "https://*/api/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_start"
    }
  ]
}
